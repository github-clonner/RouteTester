<h1><%= title %></h1>
<p>Welcome to <%= title %></p>
<form>
	<fieldset style="width:45%;height:615px;float:left;">
		<legend>Number Setup</legend>
		<ul>
			<li><label>Choose Area:</label><select id="state" onChange="popLists('city')"></select><select id="city" onChange="popLists('range')" ></select><select id="range" ></select></li>
			<li><label>Destination:</label><input type="text" id='txtDestination'></li>
			<li><label>Extension:</label><input type="text" id='txtExtension'></li>
			<li><label>DTMF for IVR (Optional):</label><input type="text" id='txtDTMF'></li>
			<li><label>DTMF Delay:</label><input type="text" id='txtDTMFDelay'></li>
			<li><label>Call Interval (in ms):</label><input type="text" id='txtInterval'></li>
			<li>
				<button id="sendCalls" type="button">Send Calls</button>
				<button id="doRange" type="button">Do Range</button>
				<button id="abortCalls" type="button">Abort Test (Hangup Calls)</button>
				<button id="listChannels" type="button">List Channels</button>
				<button id="listCommands" type="button">List Commands</button>
			</li>
		</ul>
	</fieldset>
	<fieldset style="width:45%;height:615px;float:left;">
	<legend>Channel Display</legend>
		<div id="dispChannels" style="width:100%;height:600px;float:left;overflow:scroll;">
		


		</div>
	</fieldset>
</form>

<!-- Index page logic -->
<script type="text/javascript">
	//Index Page Logic
	$('document').ready(function(){
		$( "button" ).button();
		$( '#sendCalls' ).click(function(){sendCalls();});
		$( '#doRange' ).click(function(){doRange();});
		$( '#abortCalls' ).click(function(){abortCalls();});
		$( '#listChannels' ).click(function(){listChannels();});
		$( '#listCommands' ).click(function(){listCommands();});
		popLists("state");
		


	});

</script>

